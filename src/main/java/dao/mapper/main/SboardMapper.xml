<?xml version="1.0" encoding="UTF-8"?>

<!-- 마이바티스 3 Mapper DTD -->
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="dao.face.main.SboardDao_main">
	
	<!-- 인기 스터디 목록 조회 -->
	<select id="sList" resultType="hashmap">
		SELECT S.*, M.*
		FROM studyboard S, member M
		WHERE S.member_no = M.member_no
		<![CDATA[AND rownum <= 8]]> 
	</select>

</mapper>